		
		
		<div class="col-md-3 text-center">
			<div class="col-md-8 pull-right">
			<% if @tag %>
				<div class="product_header">
					<%= @tag.tag_name %>
				</div>
				<div class="trait_on">
					<%= @tag.tag_one %>
				</div>
				<div class="trait_off">
					<%= @tag.tag_two %>
				</div>
				<div class="trait_on">
					<%= @tag.tag_three %>
				</div>
				<div class="trait_off">
					<%= @tag.tag_four %>
				</div>
				<div class="trait_on">
					<%= @tag.tag_five %>
				</div>
			<% end %>
			</div>
		</div>


		<div class="col-md-9">
			<div class="row">
				<% @quirks.each do |quirk| %>
				<div class="col-md-3 text-center">
					<div class="amazon_box">
							$10,000.00 
							<button>Buy</button>
							<br>
							^^^^^^^^
					</div>
					<div class="trait_on">
						<%= quirk.trait_one %>
					</div>
					<div class="trait_off">
						<%= quirk.trait_two %>
					</div>
					<div class="trait_on">
						<%= quirk.trait_three %>
					</div>
					<div class="trait_off">
						<%= quirk.trait_four %>
					</div>
					<div class="trait_on">
						<%= quirk.trait_five %>
					</div>
				</div>
				<% end %>
			</div>
			<div class="text-center">
				<%= will_paginate @items %>	
			</div>	
		</div>